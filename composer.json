{
  "name": "Dejabu/FlexCMS",
  "description": "Content Management System",
  "require": {
    "kriswallsmith/assetic": "^1.3",
    "gajus/dindent": "^2.0",
    "parsecsv/php-parsecsv": "^0.4",
    "illuminate/database": "^5.1",
    "illuminate/view": "^5.4",
    "cartalyst/sentinel": "^2.0",
    "baum/baum": "~1.1",
    "symfony/http-foundation": "^3.2",
    "symfony/var-dumper": "^3.1",
    "doctrine/dbal": "^2.5",
    "illuminate/pagination": "^5.3",
    "phpmailer/phpmailer": "^5.2",
    "josegonzalez/dotenv": "^2.0",
    "filp/whoops": "^2.1",
    "intervention/image": "^2.3"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master",
    "barryvdh/laravel-ide-helper": "^2.2"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.cartalyst.com"
    },
    {
      "url": "https://github.com/dosjein/external_artisan.git",
      "type": "git"
    }
  ],
  "extra": {
    "codeigniter-application-dir": "flexcms",
    "codeigniter-sparks-dir": "flexcms/sparks"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "flexcms/controllers",
      "flexcms/libraries",
      "flexcms/interfaces",
      "flexcms/modules",
      "flexcms/models",
      "flexcms/widgets",
      "flexcms/workers"
    ]
  },
  "config": {
    "vendor-dir": "flexcms/vendor"
  }
}
