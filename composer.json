{
  "name": "Dejabu/FlexCMS",
  "description": "Content Management System",
  "require": {
    "kriswallsmith/assetic": "^1.3",
    "gajus/dindent": "^2.0",
    "parsecsv/php-parsecsv": "^0.4",
    "mrclay/minify": "^2.2",
    "raveren/kint": "^1.0",
    "illuminate/database": "^5.1",
    "illuminate/view": "^5.1",
    "cartalyst/sentinel": "^2.0",
    "cartalyst/sentinel-social": "^2.0",
    "cartalyst/interpret": "^1.1",
    "leafo/scssphp": "^0.5",
    "leafo/lessphp": "^0.5",
    "cartalyst/nested-sets": "^3.0",
    "cartalyst/themes": "^3.0",
    "symfony/http-foundation": "^2.7",
    "symfony/var-dumper": "^3.1",
    "doctrine/dbal": "^2.5",
    "cartalyst/assetic-filters": "^1.2",
    "twig/twig": "^1.27",
    "illuminate/pagination": "^5.3",
    "vlucas/phpdotenv": "^2.4"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.cartalyst.com"
    }
  ],
  "extra": {
    "codeigniter-application-dir": "framework/application",
    "codeigniter-sparks-dir": "framework/sparks"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "framework/application/controllers",
      "framework/application/libraries",
      "framework/application/interfaces",
      "framework/application/modules",
      "framework/application/models",
      "framework/application/widgets",
      "framework/application/workers"
    ]
  },
  "config": {
    "vendor-dir": "framework/vendor"
  }
}
