{
  "name": "bxSlider",
  "version": "4.2.1",
  "assets": {
    "prod": {
      "css": ["packages/bx-slider.js/dist/jquery.bxslider.css"],
      "js": ["packages/bx-slider.js/dist/jquery.bxslider.js"]
    },
    "dev": {
      "css": ["packages/bx-slider.js/src/less/jquery.bxslider.less"],
      "js": ["packages/bx-slider.js/src/js/jquery.bxslider.js"]
    }
  },
  "docs": "http://bxslider.com/options",
  "config": {
    "mode": ["horizontal", "vertical", "fade"],
    "speed": 500,
    "slideMargin": 0,
    "startSlide": 0,
    "randomStart": [false, true],
    "infiniteLoop": [true, false],
    "hideControlOnEnd": [false, true],
    "easing": ["linear", "ease", "ease-in", "ease-out", "ease-in-out"],
    "captions": [false, true],
    "ticker": [false, true],
    "tickerHover": [false, true],
    "adaptiveHeight": [true, false],
    "adaptiveHeightSpeed": 500,
    "video": [true, false],
    "preloadImages": ["all", "visible"],
    "pager": [true, false],
    "pagerType": ["full", "short"],
    "pagerShortSeparator": " / ",
    "controls": [true, false],
    "nextText": "Next",
    "prevText": "Prev",
    "autoControls": [false, true],
    "startText": "Start",
    "stopText": "Stop",
    "auto": [true, false],
    "pause": 8000,
    "autoStart": [true, false],
    "autoDirection": ["next", "prev"],
    "autoHover": [false, true],
    "autoDelay": 0,
    "minSlides": 1,
    "maxSlides": 1,
    "moveSlides": 0,
    "slideWidth": 0
  }
}